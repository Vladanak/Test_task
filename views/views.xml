<?xml version="1.0" encoding="utf-8"?>

<odoo>
     <record id="manufactory_config_action" model="ir.actions.act_window">
        <field name="name">sales.brand.name</field>
         <field name="res_model">sales.brand</field>
         <field name="type">ir.actions.act_window</field>
         <field name="view_type">form</field>
    </record>


    <record id="module_config_action" model="ir.actions.act_window">
        <field name="name">sales.model.name</field>
         <field name="res_model">sales.model</field>
         <field name="type">ir.actions.act_window</field>
         <field name="view_type">form</field>
    </record>


    <menuitem id="menu_sales_manufactory_config"
            name="Manufactory"
            parent="sale.menu_sale_config"
            action="manufactory_config_action"
            sequence="8"/>


    <menuitem id="menu_sales_module_config"
            name="Module"
            parent="sale.menu_sale_config"
            action="module_config_action"
            sequence="9"/>

</odoo>